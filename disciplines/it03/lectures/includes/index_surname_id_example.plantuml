@startuml Пример таблицы и индекса по фамилии
top to bottom direction
skinparam linetype ortho
skinparam defaultTextAlignment center
skinparam rectangle {
  BackgroundColor #efefef
  BorderColor #666666
  RoundCorner 8
}
skinparam ArrowColor #666666

rectangle "Индекс по фамилии" as IDX #fcfcfc {
  rectangle "Андреев" as L1
  rectangle "Белов" as L2
  rectangle "Иванов" as L3
  rectangle "Кузнецов" as L4
  rectangle "Орлова" as L5
  rectangle "Петров" as L6
  rectangle "Сидоров" as L7

  L1 -[hidden]down-> L2
  L2 -[hidden]down-> L3
  L3 -[hidden]down-> L4
  L4 -[hidden]down-> L5
  L5 -[hidden]down-> L6
  L6 -[hidden]down-> L7
}

rectangle "Ссылки на ключи" as TBL #fcfcfc {
  rectangle "id=2" as R1
  rectangle "id=4" as R2
  rectangle "id=6" as R3
  rectangle "id=7" as R4
  rectangle "id=3" as R5
  rectangle "id=5" as R6
  rectangle "id=1" as R7

  R1 -[hidden]down-> R2
  R2 -[hidden]down-> R3
  R3 -[hidden]down-> R4
  R4 -[hidden]down-> R5
  R5 -[hidden]down-> R6
  R6 -[hidden]down-> R7
}

L1 -[norank]-> R1
L2 -[norank]-> R2
L3 -[norank]-> R3
L4 -[norank]-> R4
L5 -[norank]-> R5
L6 -[norank]-> R6
L7 -[norank]-> R7
@enduml
